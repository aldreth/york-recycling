    "react-scripts": "3.4.1",
    "redux-persist": "6.0.0",
    "serve": "11.3.2",
    "typescript": "3.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run lint && react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run tsc && eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
