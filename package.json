  "author": "Edward Andrews-Hodgson",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/auto-id": "0.10.3",
    "@reach/dialog": "0.10.4",
    "@reduxjs/toolkit": "1.3.6",
    "@types/classnames": "2.2.10",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
